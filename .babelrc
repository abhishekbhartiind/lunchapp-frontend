{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"browsers": [
						"> 1%",
						"last 2 versions"
					]
				},
				"modules": "commonjs"
			}
		],
		"@babel/preset-flow",
		"@babel/preset-react",
	],
	"plugins": [
		[
			"import",
			{
				"libraryName": "@digihcs/innos-ui3",
				"libraryDirectory": "es",
				"style": "true",
			},
			"@digihcs/innos-ui3",
		]
	]
}
